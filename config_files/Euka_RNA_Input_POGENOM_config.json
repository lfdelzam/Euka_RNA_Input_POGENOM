{
  "workdir": "/absolute/path/to/Euka_RNA_Input_POGENOM",
  "dataset": "NAME",
  "min_coverage": 1,
  "min_breadth": 0.001,
  "min_bsq_for_cov_median_calculation": 15,
  "mode": "prefilt",
  "fraction": "0.10",
  "temp_sub_Reads_dir": "PREFILT",
  "remove_subreads": "no",
  "threads": 15,
  "genomes_ext": ".fa",
  "reads_ext": ".fq.gz",
  "fwd_index": "_R1",
  "rev_index": "_R2",
  "hisat2_params": "--ignore-quals --mp 1,1 --np 1 --rdg 0,1 --rfg 0,1 --score-min L,0,-0.05 --min-intronlen 20 --max-intronlen 1000",
  "mapqual": 20,
  "samtools_view_alignment_extra_filters": "-f 2 -F 1024",
  "read_counts_per_gene": "yes",
  "htseq_params": "-s no -t CDS -i ID -m intersection-nonempty",
  "freebayes_parameters": "-C 4 -p 1 --pooled-continuous --read-max-mismatch-fraction 0.05 --min-alternate-fraction 0.01 -q 15",
  "vcffilter_qual": "'QUAL > 20'",
  "snakemake_extra_params": ""
}
