{
  "workdir": "/abs/path/to/Euka_RNA_Input_POGENOM",
  "dataset": "TEST",
  "min_coverage": 10,
  "min_breadth": 10000,
  "min_bsq_for_cov_median_calculation": 15,
  "mode_prefilt": "TRUE",
  "fraction": "0.15",
  "temp_sub_Reads_dir": "PREFILT",
  "remove_subreads": "FALSE",
  "threads": 20,
  "genomes_ext": ".fa",
  "reads_ext": ".fq.gz",
  "fwd_index": "_R1",
  "rev_index": "_R2",
  "hisat2_params": "--no-mixed --ignore-quals --mp 1,1 --np 1 --rdg 0,1 --rfg 0,1 --score-min L,0,-0.03 --min-intronlen 20 --max-intronlen 500000",
  "mapqual": 20,
  "samtools_view_alignment_extra_filters": "-f 2 -F 3084",
  "read_counts_per_gene": "FALSE",
  "htseq_params": "-s no -t exon -i ID -m union --nonunique all --supplementary-alignments ignore",
  "freebayes_parameters": "-C 4 -p 1 --pooled-continuous --read-max-mismatch-fraction 0.05 --min-alternate-fraction 0.01 -q 15 --report-monomorphic",
  "vcffilter_qual": "'QUAL > 20'",
  "snakemake_extra_params": ""
}
